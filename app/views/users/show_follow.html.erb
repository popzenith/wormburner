<div class="span10 offset1">
<h2><%= @user.name %> : <%= @title %></h2>

<%= render 'layouts/messages' %>

<ul class="users">
  <% if @users.any? %>
  	<%= will_paginate %>
	    <% @users.each do |user| %>
	    <li>
	      <%= link_to user.name, user %> 
	    </li>
	    <% end %>
    <%= will_paginate %>
  <% end %>
</ul>
</div>