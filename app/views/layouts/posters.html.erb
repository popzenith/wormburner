<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title><%= content_for?(:title) ? yield(:title) : "Data Visualization Posters for the 2015 Major Winners" %></title>
    <meta name="google-site-verification" content="VnHgMjgi9aFxE9FZY1JUYDxEfu5D2euIbHOxx4LfMkg" />
    <meta name="description" content="WormBurner is a new stats and scoring app for golfers.">
    <meta name="keywords" content="golf, scorecard, scoring, statistics, visualization, app, golfers, golfing, course, tee times">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <link href='http://fonts.googleapis.com/css?family=Lato:400,700' rel='stylesheet' type='text/css'>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <%= yield(:head) %>
  </head>
  <body>
    <div id="sitewidth"></div> 
      <%= render 'layouts/header' %>
    <div class="maincontainer">
      <div id="main" role="main">
        <div class="container">
          <div class="row">dds
            <%= yield %>
          </div>
        </div> 
      </div> 
    </div>
    <div class="footercontainer">
      <div class="container">
        <div class="row">
          <div class="span10 offset1">
            <footer class="footer">
              <div id="footerright">
                &copy; <%= copyright_notice_year %> WormBurner. All Rights Reserved.  
              </div>
              <div id="footerleft">
                <a href="/contact">Contact us</a> anytime with questions or comments.
              </div>
            </footer>
          </div> 
        </div> 
      </div> 
    </div>
  </body>
</html>